{
  "name": "@masochistme/api",
  "version": "3.2.0",
  "description": "MasochistME API Service",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "api:dev": "env ENV=dev PORT=3002 CORS=http://masochist.me DELAY=3000 BIG_DELAY=43200000 npx tsx lib/index.ts",
    "api:dev:watch": "env ENV=dev PORT=3002 CORS=http://masochist.me DELAY=3000 BIG_DELAY=43200000 nodemon --exec npx tsx lib/index.ts",
    "api:prod": "env ENV=prod PORT=3002 CORS=http://masochist.me DELAY=3000 BIG_DELAY=43200000 npx tsx lib/index.ts",
    "api:prod:watch": "env ENV=prod PORT=3002 CORS=http://masochist.me DELAY=3000 BIG_DELAY=43200000 nodemon --exec npx tsx lib/index.ts",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix --quiet"
  },
  "author": "Arcyvilk",
  "license": "ISC",
  "dependencies": {
    "@masochistme/sdk": "*",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongo": "^0.1.0",
    "mongodb": "^4.10.0",
    "passport": "^0.6.0",
    "passport-steam": "^1.0.15"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.42",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.186",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lerna": "^5.6.2",
    "lodash": "^4.17.13",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PointOnePercent/dds-microservice.git"
  },
  "bugs": {
    "url": "https://github.com/PointOnePercent/dds-microservice/issues"
  },
  "homepage": "https://github.com/PointOnePercent/dds-microservice#readme",
  "workspaces": [
    "packages/*"
  ]
}
